{
    "name": "cj-bigdevil/chartjs",
    "description": "Use Chartjs in laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "chartjs"],
    "homepage": "https://github.com/CJ-BigDevil/chartjs",
    "license": "MIT",
    "authors": [
        {
            "name": "CJ",
            "email": "shigaiyan18@163.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Chartjs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Chartjs\\ChartjsServiceProvider"
            ]

        }
    }
}
